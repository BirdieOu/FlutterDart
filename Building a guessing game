import "dart:math";
import "dart:io";

String prompt(String promptText) {
  print(promptText);
  String answer = stdin.readLineSync();
  return answer;
}

double promptDouble() {
}


void main() {
  
  String answer = "john wick";
  String guess = "";
  int guessCount = 0;

// while loop
  while(guess != answer) {
    guess = prompt("Enter a guess:");
    guessCount++;

  }

  print("You won in ${guessCount} guesses!");

}


